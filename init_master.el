  (setq frame-title-format "Guoqiang Jin@%b")
  (setq user-full-name "Guoqiang Jin")
  (setq user-mail-address "ustczhan@gmail.com")
  (setq configuration-layer--elpa-archives
        ;;'(("melpa-cn" . "http://elpa.zilongshanren.com/melpa/")
